        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Modality"/>
        <rdfs:comment>Natural language processing has seen all classes of adversarial attacks, including evasion, poisoning, and privacy breaches.</rdfs:comment>
        <rdfs:label>Text</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#The_Tail -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#The_Tail">
