    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Governance -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Governance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#EU_AI_Act_Rights"/>
        <rdfs:comment>High-quality data sets must be used in the development of AI systems to prevent biases that could lead to discrimination. Individuals have the right to expect that AI systems are free from discrimination and that the data used to train these systems is handled responsibly.</rdfs:comment>
        <rdfs:label>Data Governance</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Inputs -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Inputs">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#AI_system."/>
        <rdfs:comment>Description of data inputs to the technology</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Leakage -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Leakage">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Misconfiguration"/>
        <dc:rhudock>rhudock</dc:rhudock>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Management_and_Analytics -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Management_and_Analytics">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Use_Case_Definition"/>
        <rdfs:label>Data Management and Analytics</rdfs:label>
        <rdfs:seeAlso>https://ai.gov/ai-use-cases/</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Point_Disclosure -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Point_Disclosure">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Transparency_of_impact"/>
        <rdfs:comment>Communicate information about the type of data points the algorithm uses.</rdfs:comment>
        <rdfs:label>Data Point Disclosure</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Poisoning -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Poisoning">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Poisoning"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Threat"/>
        <dc:rhudock>rhudock</dc:rhudock>
        <rdfs:comment>Introducing malicious data into the training set.</rdfs:comment>
        <rdfs:label>Data Poisoning Attacks</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Reconstruction -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Reconstruction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Privacy_Violation"/>
        <ks:cite>NIST AI 100-2e2023 (January 2024)</ks:cite>
        <rdfs:comment>Learning about the training data.</rdfs:comment>
        <rdfs:label>Data Reconstruction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Robustness -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Robustness">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Using_Artificial_Intelligence_and_Algorithms"/>
        <rdfs:comment>Companies need to ensure their data is accurate and up-to-date, especially if used to make critical decisions about consumers. AI models should be validated and revalidated for effectiveness and fairness.</rdfs:comment>
        <rdfs:label>Data Robustness</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Sanitization -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Sanitization">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Mitigations"/>
