    <!-- http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Flow_Diagram -->

    <owl:Class rdf:about="http://www.semanticweb.org/rhudock/ontologies/2023/6/ai-risk-compliance-ontology#Data_Flow_Diagram">
